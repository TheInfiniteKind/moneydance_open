      classpathref="classpath"
      classname="com.moneydance.admin.KeyAdmin"
      fork="true">
      <sysproperty key="moneydance_key_pass" value="${keypass}"/>
      <arg value="signextjar"/>
      <arg value="${extprivkeyfile}"/>
      <arg value="${privkeyid}"/>
      <arg value="${feature}"/>
